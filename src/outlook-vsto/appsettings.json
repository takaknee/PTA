{
  "OpenAI": {
    "ApiKey": "YOUR_API_KEY_HERE",
    "Endpoint": "https://your-azure-openai-endpoint.openai.azure.com/openai/deployments/gpt-4/chat/completions?api-version=2024-02-15-preview",
    "Model": "gpt-4",
    "MaxTokens": 2000,
    "Temperature": 0.7,
    "TimeoutSeconds": 30
  },
  "Claude": {
    "ApiKey": "YOUR_CLAUDE_API_KEY_HERE",
    "Model": "claude-3-sonnet-20240229",
    "MaxTokens": 2000,
    "TimeoutSeconds": 30
  },
  "Graph": {
    "ClientId": "YOUR_CLIENT_ID_HERE",
    "ClientSecret": "YOUR_CLIENT_SECRET_HERE",
    "TenantId": "YOUR_TENANT_ID_HERE",
    "Scopes": [
      "https://graph.microsoft.com/Mail.ReadWrite",
      "https://graph.microsoft.com/Calendars.ReadWrite",
      "https://graph.microsoft.com/Sites.ReadWrite.All",
      "https://graph.microsoft.com/Group.ReadWrite.All"
    ]
  },
  "Integration": {
    "DefaultTeamId": "YOUR_DEFAULT_TEAM_ID",
    "DefaultChannelId": "YOUR_DEFAULT_CHANNEL_ID",
    "DefaultSharePointSiteId": "YOUR_SHAREPOINT_SITE_ID",
    "DefaultListId": "YOUR_LIST_ID",
    "EnableTeamsIntegration": false,
    "EnableSharePointIntegration": false,
    "EnableCalendarIntegration": false
  },
  "Application": {
    "Name": "Outlook AI Helper",
    "Version": "1.0.0 VSTO",
    "MaxContentLength": 50000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "OutlookPTAAddin": "Debug"
    },
    "File": {
      "Enabled": true,
      "FileName": "PTA_Outlook_Addin.log",
      "MaxFileSizeMB": 10,
      "MaxFiles": 5
    }
  },
  "Features": {
    "AutoUpdate": {
      "Enabled": true,
      "CheckIntervalDays": 7,
      "UpdateUrl": ""
    },
    "Telemetry": {
      "Enabled": false,
      "CollectUsageStats": false
    },
    "UI": {
      "ShowProgressDialogs": true,
      "DefaultTimeout": 30,
      "ShowTaskPane": true,
      "EnableAdvancedDialogs": true
    }
  }
}